{
	"name": "v3-gmaps",
	"version": "1.0.0",
	"private": false,
	"description": "A lightweight, typed, Google Maps plugin for Vue 3",
	"author": "Keagan Chisnall",
	"type": "module",
	"module": "./dist/v3-gmaps.js",
	"main": "./dist/v3-gmaps.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.js",
		"dist/*.css",
		"dist/**/*.d.ts"
	],
	"scripts": {
		"dev": "cd demo && vite --host",
		"build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
		"test": "vitest"
	},
	"devDependencies": {
		"@babel/types": "^7.27.0",
		"@types/google.maps": "^3.58.1",
		"@types/node": "^22.14.0",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vue/compiler-sfc": "^3.5.13",
		"@vue/test-utils": "^2.4.6",
		"typescript": "^5.8.2",
		"vite": "^6.2.3",
		"vitest": "^3.0.9",
		"vue": "^3.5.13",
		"vue-tsc": "^2.2.8"
	},
	"bugs": {
		"url": "https://github.com/xon52/v3-gmaps/issues"
	},
	"homepage": "https://github.com/xon52/v3-gmaps#readme",
	"keywords": [
		"vue",
		"plugin",
		"map",
		"google",
		"typescript"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xon52/v3-gmaps.git"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		]
	}
}
