{
	"name": "v3-gmaps",
	"version": "1.0.0",
	"description": "Lightweight, typed, Google Maps components for Vue 3",
	"author": "Keagan Chisnall",
	"type": "module",
	"module": "./dist/v3-gmaps.js",
	"main": "./dist/v3-gmaps.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "cd demo && vite --host",
		"build": "vite build",
		"test": "vitest",
		"pack": "rm -f v3-gmaps.tgz && pnpm build && pnpm pack && mv v3-gmaps-*.tgz v3-gmaps.tgz && rm -rf dist",
		"docs:dev": "cd docs && vitepress dev",
		"docs:build": "cd docs && vitepress build"
	},
	"devDependencies": {
		"@types/google.maps": "^3.58.1",
		"@types/node": "^22.14.1",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vue/test-utils": "^2.4.6",
		"typescript": "^5.8.3",
		"vite": "^6.3.2",
		"vite-plugin-dts": "^4.5.3",
		"vitepress": "^1.6.3",
		"vitest": "^3.1.1"
	},
	"peerDependencies": {
		"vue": "^3.5.13"
	},
	"bugs": {
		"url": "https://github.com/xon52/v3-gmaps/issues"
	},
	"homepage": "https://github.com/xon52/v3-gmaps#readme",
	"keywords": [
		"vue",
		"plugin",
		"map",
		"google",
		"typescript"
	],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xon52/v3-gmaps.git"
	},
	"pnpm": {
		"ignoredBuiltDependencies": [
			"esbuild"
		]
	}
}
